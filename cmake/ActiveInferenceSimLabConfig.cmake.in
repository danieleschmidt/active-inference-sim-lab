@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find dependencies
find_dependency(Eigen3 3.3 REQUIRED)
find_dependency(Threads REQUIRED)

# Include targets
include("${CMAKE_CURRENT_LIST_DIR}/ActiveInferenceSimLabTargets.cmake")

# Set variables
set(ActiveInferenceSimLab_FOUND TRUE)
set(ActiveInferenceSimLab_VERSION @PROJECT_VERSION@)
set(ActiveInferenceSimLab_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(ActiveInferenceSimLab_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(ActiveInferenceSimLab_VERSION_PATCH @PROJECT_VERSION_PATCH@)

# Check if all required components are found
check_required_components(ActiveInferenceSimLab)